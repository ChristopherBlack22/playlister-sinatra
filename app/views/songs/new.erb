<h1>Create a new Song</h1>
<form method="post" action="/songs">
    <label>Name: </label>
    <input type="text" name="song[name]" id="Name"><br><br>
    <label>Artist: </label>
    <input type="text" name="artist[name]" id="Artist Name"><br><br>
    <label>Genres: </label>
    <%@genres.each do |genre|%>
        <input type="checkbox" name="genres[]" id="<%=genre.name%>" value="<%=genre.id%>"><%=genre.name%>
    <%end%><br><br>
    <input type="submit" id="Create" value="Create">
</form>